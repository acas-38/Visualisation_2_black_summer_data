{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 950,
  "height": 700,
  "title": "AUS GEEBAM Burnt Areas",
  "projection": {"type": "mercator", "center": [135, -30], "scale": 800},
  "data": {
    "url": "https://raw.githubusercontent.com/acas-38/Visualisation_2_black_summer_data/main/burnt_area_small.geojson",
    "format": {"type": "json", "property": "features"}
  },
  "transform": [
    {"filter": "datum.properties.DN >= 11 && datum.properties.DN <= 24"}
  ],
  "mark": {
    "type": "geoshape",
    "fill": "none",
    "stroke": "black",
    "strokeWidth": 0.7
  },
  "config": {"view": {"stroke": null}}
}

